# Makefile.in generated by hand.
# @configure_input@

CC = @CC@
CFLAGS = @CFLAGS@
CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
CXX = @CXX@
CXXLD = $(CXX)
CXXCPP = @CXXCPP@
CXXFLAGS = @CXXFLAGS@ -O3 -Wall -Wextra -fPIC
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
MKDIR_P = @MKDIR_P@

EXEEXT = @EXEEXT@
OBJEXT = @OBJEXT@

$(shell $(MKDIR_P) bin)

all: bin/threes$(EXEEXT) bin/threes.so

bin/%$(EXEEXT): bin/%.$(OBJEXT)
	$(CXXLD) $(CXXFLAGS) $(LDFLAGS) $^ $(LDLIBS) -o $@

bin/%.so: bin/%.$(OBJEXT)
	$(CXXLD) $(CXXFLAGS) -shared $(LDFLAGS) $^ $(LDLIBS) -o $@

bin/%.$(OBJEXT) : threes_cpp/%.cpp
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c -o $@ $<

clean:
	# C++ build files
	$(RM) -rf bin/

cleanall:
	clean
	
	# Python build files
	$(RM) -rf build/ 
	
	# Autoconf files
	$(RM) -rf autom4te.cache/
	$(RM) aclocal.m4 configure configure~ install-sh install-sh~ 
	$(RM) config.h config.h.in~ config.h.in

	# Configure files
	$(RM) config.log config.status Makefile

.PHONY: all clean
